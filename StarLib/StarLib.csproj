<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>CosmicCalendar</AssemblyName>
    <RootNamespace>CosmicCalendar</RootNamespace>
    <Authors>Silicon Prophet</Authors>
    <Company>Order of Life</Company>
    <Product>CosmicCalendar</Product>
    <Description>The Cosmic Calendar of the Order of Life</Description>
    <Copyright>Order of Life 2020</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/siliconprophet/CosmicCalendar</RepositoryUrl>
    <RepositoryType>Library</RepositoryType>
    <PackageTags>Order of Life, Calendar</PackageTags>
    <PackageReleaseNotes>Switched to .Net Core</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
    <PackageId>StarDate</PackageId>
    <PackageIcon>DNA Tree Logo.jpg</PackageIcon>
    <NeutralLanguage></NeutralLanguage>
    <PackageProjectUrl>https://github.com/siliconprophet/CosmicCalendar</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Dropbox\branding images\DNA Tree Logo.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\CosmicCalendar\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.PowerShell.5.ReferenceAssemblies" Version="1.1.0" />
  </ItemGroup>

</Project>
